        <div class="respuesta">
            <h3 class="center_text"> Tu respuesta </h3>
            <div id="contentrespuesta" class="center_text contentrespuesta">
                {{!-- {{#each this.pos}} --}}

                <input class="resinput" value="" id="res{{@index}}" type="{{this.oktype}}">
                {{!-- {{/each}} --}}

            </div>
            <div id="validar" class="center_text ">
                <button id="enviarinfo" class="center_text btn_res" type="submit">Enviar</button> </div>
