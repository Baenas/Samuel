    <h1 id="p1Name" class=" titletopmain center_text"> <span>{{this.id}}</span> | Pregunta
    </h1>
    <div class="contenedor1">
        <div class="tools">
            <div id="preg_1" class="center_text itm_2">{{this.pregunta}}</div>
            <div id="respuestanow" hidden class="center_text itm_2">{{this.ok}}</div>
            <div id="itm_1_1" class="center_text itm_1">{{this.pos.p1}}</div>
            <div id="itm_1_2" class="center_text itm_1">{{this.pos.p1_2}}</div>
